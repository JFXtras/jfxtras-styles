<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="600.0" prefWidth="500" styleClass="background" xmlns:fx="http://javafx.com/fxml"
            fx:controller="jfxtras.styles.samples.jmetro.controlssample.SpinnerController">
    <Label layoutX="43.0" layoutY="37.0" styleClass="header" text="Spinner">
    </Label>

    <Label layoutX="43.0" layoutY="165.0" styleClass="item-title" text="SPLIT_ARROWS_HORIZONTAL" textFill="WHITE">
    </Label>
    <Spinner layoutX="43.0" layoutY="187.0" fx:id="spinner1">
        <styleClass>
            <String fx:value="split-arrows-horizontal"/>
        </styleClass>
    </Spinner>

    <Label layoutX="260.0" layoutY="165.0" styleClass="item-title" text="ARROWS_ON_RIGHT_HORIZONTAL">
    </Label>
    <Spinner layoutX="260.0" layoutY="187.0" fx:id="spinner2">
        <styleClass>
            <String fx:value="arrows-on-right-horizontal"/>
        </styleClass>
    </Spinner>

    <Label layoutX="43.0" layoutY="265.0" styleClass="item-title" text="No Styleclass" textFill="WHITE">
    </Label>
    <Spinner layoutX="43.0" layoutY="287.0" fx:id="spinner3">
    </Spinner>

    <Label layoutX="260.0" layoutY="265.0" styleClass="item-title" text="ARROWS_ON_LEFT_HORIZONTAL">
    </Label>
    <Spinner layoutX="260.0" layoutY="287.0" prefWidth="70" fx:id="spinner4">
        <styleClass>
            <String fx:value="arrows-on-left-horizontal"/>
        </styleClass>
    </Spinner>

    <Label layoutX="43.0" layoutY="365.0" styleClass="item-title" text="ARROWS_ON_LEFT_VERTICAL">
    </Label>
    <Spinner layoutX="43.0" layoutY="387.0" prefWidth="50" fx:id="spinner5">
        <styleClass>
            <String fx:value="arrows-on-left-vertical"/>
        </styleClass>
    </Spinner>

    <Label layoutX="260.0" layoutY="365.0" styleClass="item-title" text="SPLIT_ARROWS_VERTICAL">
    </Label>
    <Spinner layoutX="260.0" layoutY="387.0" prefWidth="35" fx:id="spinner6">
        <styleClass>
            <String fx:value="split-arrows-vertical"/>
        </styleClass>
    </Spinner>

    <Label layoutX="43.0" layoutY="465.0" styleClass="item-title" text="Disabled">
    </Label>
    <Spinner disable="true" layoutX="43.0" layoutY="487.0" fx:id="disabledSpinner">
    </Spinner>
</AnchorPane>
